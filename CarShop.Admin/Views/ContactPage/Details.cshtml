@model CarShop.Model.ContactPage

@{
    ViewBag.Title = "İletişim Sayfası Detaylar";
}

<h2>Detaylar</h2>

<div>
    <hr />
    <dl class="dl-horizontal">

        <dt>
            @Html.DisplayNameFor(model => model.ContactPageAddress)
        </dt>
        <dd>
            @Html.Raw(Model.ContactPageAddress)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.ContactPageMarketingPhone)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ContactPageMarketingPhone)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.ContactPageMarketingEmail)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ContactPageMarketingEmail)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.ContactPageShippingPhone)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ContactPageShippingPhone)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.ContactPageShippingEmail)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ContactPageShippingEmail)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.ContactPageInformationPhone)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ContactPageInformationPhone)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.ContactPageInformationEmail)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ContactPageInformationEmail)
        </dd>

        <dt>
            <label for="Uploads">
                İletişim Sayfası Fotoğrafı
            </label>
        </dt>
        <dd>

            <a href="@(ViewBag.AssetsUrl)@Model.ContactPagePhoto">@Model.ContactPagePhoto</a><br />

        </dd>



    </dl>
</div>
<p>
    @Html.ActionLink("Düzenle", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("Geriye Dön", "Index")
</p>
